{
  "name": "@lyavon/es6_crypto",
  "version": "1.0.1",
  "description": "Convenient secp256r1 wrapper for srypto.subtle",
  "license": {
    "type": "GPL-3.0",
    "url": "https://www.gnu.org/licenses/gpl-3.0.html"
  },
  "author": "Leonid Ragunovich <leonid.ragunovich@gmail.com>",
  "type": "module",
  "keywords": [
    "P-256",
    "crypto",
    "crypto.subtle",
    "cryptography",
    "secp256r1",
    "subtle"
  ],
  "scripts": {
    "documentation": "jsdoc -p -r -d ./documentation -R ./README.md ./src",
    "show-documentation": "sh ./scripts/show-documentation.sh",
