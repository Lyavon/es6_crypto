    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "exports": {
    ".": {
      "default": "./src/crypto.js"
    }
  },
  "devDependencies": {
    "jest": "",
    "jsdoc": "",
    "standard": ""
  },
  "repository": {
    "type": "git",
